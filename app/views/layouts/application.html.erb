    <link rel="shortcut icon" type="image/x-icon" href="../assets/images/favicon.ico">
 </head>

<body>
<%= render 'member/layouts/header' %>
<div>
  <% flash.each do |name, msg| %>
      <%= content_tag(:div, msg, class: "alert alert-info") %>
  <% end %>
  <%= yield %>
</div>
</body>
</html>